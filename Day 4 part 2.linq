	var rows = input.Split(Environment.NewLine);
	
	var numMatches = 0;
	for(var x = 0; x < rows.Length; ++x)
	{
		for(var y = 0; y < rows[x].Length - 2; ++y)
		{
			//one diag down right + one diag down left on y+2
			var match = NumImmediateMatches(rows, x, y, "MAS", Direction.DiagonalDownRight) + NumImmediateMatches(rows, x, y, "SAM", Direction.DiagonalDownRight) > 0;
			var xMatch = NumImmediateMatches(rows, x, y + 2, "MAS", Direction.DiagonalDownLeft) + NumImmediateMatches(rows, x, y + 2, "SAM", Direction.DiagonalDownLeft) > 0;
			
			if (match && xMatch)
			{
				numMatches++;
			}
		}
	}
	
	Console.Out.WriteLine(numMatches);
}

private int NumImmediateMatches(string[] rows, int x, int y, string wordToMatch, Direction direction)
{
	var firstCharToMatch = wordToMatch.First();
	var c = rows[x][y];

	if (c != firstCharToMatch)
	{
		return 0;
	}

	if (wordToMatch.Length == 1)
	{
		return 1;
	}

	var numMatches = 0;

	if (x < rows.Length - 1 && direction == Direction.VerticalDown)
	{
		numMatches += NumImmediateMatches(rows, x + 1, y, wordToMatch.Substring(1), direction);
	}
	
	if (x > 0 && direction == Direction.VerticalUp)
	{
		numMatches += NumImmediateMatches(rows, x - 1, y, wordToMatch.Substring(1), direction);
	}
	
	if (y < rows[x].Length - 1 && direction == Direction.HorizontalRight)
	{
		numMatches += NumImmediateMatches(rows, x, y + 1, wordToMatch.Substring(1), direction);
	}
	
	if (y > 0 && direction == Direction.HorizontalLeft)
	{
		numMatches += NumImmediateMatches(rows, x, y - 1, wordToMatch.Substring(1), direction);
	}
	
	if (x < rows.Length - 1 && y < rows[x].Length - 1 && direction == Direction.DiagonalDownRight)
	{
		numMatches += NumImmediateMatches(rows, x + 1, y + 1, wordToMatch.Substring(1), direction);
	}
	
	if (x < rows.Length - 1 && y > 0 && direction == Direction.DiagonalDownLeft)
	{
		numMatches += NumImmediateMatches(rows, x + 1, y - 1, wordToMatch.Substring(1), direction);
	}
	
	if (x > 0 && y > 0 && direction == Direction.DiagonalUpLeft)
	{
		numMatches += NumImmediateMatches(rows, x - 1, y - 1, wordToMatch.Substring(1), direction);
	}
	
	if (x > 0 && y < rows[x].Length - 1 && direction == Direction.DiagonalUpRight)
	{
		numMatches += NumImmediateMatches(rows, x - 1, y + 1, wordToMatch.Substring(1), direction);
	}
	
	return numMatches;
}

private enum Direction
{
	HorizontalRight,
	VerticalDown,
	DiagonalDownLeft,
	DiagonalDownRight,
	HorizontalLeft,
	VerticalUp,
	DiagonalUpRight,
	DiagonalUpLeft,
}
